    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.10.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "apexcharts": "^3.26.1",
    "autoprefixer": "9",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "craco-antd": "^1.19.0",
    "js-cookie": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "mockjs": "^1.1.0",
    "node-sass": "4.14.1",
